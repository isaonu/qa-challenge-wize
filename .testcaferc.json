{
    "browsers": "chrome",
    "src": "POM/test/*",
    "screenshots": {
        "path": "./results/screenshots/",
        "takeOnFails": false,
        "fullPage": false
    },
    "reporter": [
        {
        "name": "xunit",
        "output": "results/report.xml"
        },
        {
        "name": "json",
        "output": "results/report.json"
        }
        ]
    ,
    "skipJsErros": true,
    "selectorTimeOut": 5000,
    "assertionTimeout": 4000,
    "pageLoadTimeout": 4000,
    "speed": 1
}